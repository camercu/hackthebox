	set +e; $SELF stop; set -e
	$SELF start
  'reload'|'force-reload')
  	log_daemon_msg "Reloading MySQL database server" "mysqld"
	$MYADMIN reload
	log_end_msg 0
  'status')
	if mysqld_status check_alive nowarn; then
